{
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:prettier/recommended"
	],
	"plugins": ["react"],
	"env": {
		"browser": true,
		"es6": true
	},
	"globals": {
		"Atomic": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"rules": {
		"prettier/prettier": [
			"error",
			{
				"singleQuote": false
			}
		],
		"no-console": "off",
		"indent": ["4", "tab", { "SwitchCase": 1 }]
	}
}
