{
	"extends": ["eslint:recommended", "plugin:react/recommended"],
	"plugins": ["react"],
	"env": {
		"browser": true,
		"es6": true
	},
	"globals": {
		"Atomic": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"rules": {
		"no-console": "off",
		"indent": [0]
	}
}
